<launch>
    <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find orbbec_camera)/urdf/test_astra2.urdf.xarco' use_nominal_extrinsics:=true add_plug:=true use_mesh:=true" />
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />

    <arg name="gui" default="True" />
    <param name="use_gui" value="$(arg gui)" />
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find orbbec_camera)/rviz/urdf.rviz" required="true" />
</launch>
